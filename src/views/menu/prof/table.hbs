        </div>
    </div>
    <table class="table table-light table-hover">
        <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Player Login</th>
              <th scope="col">Winrate</th>
              <th scope="col">Game count</th>
            </tr>
          </thead>
          <tbody>
            {{#each users}}
